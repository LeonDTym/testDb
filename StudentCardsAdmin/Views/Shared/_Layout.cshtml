                        </li>*@
                        <li style="margin-right:15px;" class="nav-item">
                          @* <div>
                                <label style="margin-top: 10px;">@Context.Session.GetString("Name")</label>
                                <label style="">@Context.Session.GetString("ObjName")</label>
                           </div>*@
                           <div>
                                <div style="" class="">@Context.Session.GetString("Name") <p style="font-size:12px;margin-left:;margin-bottom:0px;text-align:right" class="">@Context.Session.GetString("ObjName")</p></div>
                                
                            </div>
                        </li>
                         <li style="margin-right:15px;" class="nav-item">
